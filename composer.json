{
    "name": "carsguide/recaptcha",
    "description": "ReCaptcha middleware for Laravel",
    "keywords": [
        "ReCaptcha",
        "Lumen"
    ],
    "homepage": "https://github.com/carsguide/recaptcha",
    "license": "MIT",
    "authors": [
        {
            "name": "Najji Muhammed",
            "email": "muhammed.najji@carsguide.com.au"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/http": "^6.0",
        "google/recaptcha": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Carsguide\\ReCaptcha\\" : "src/" 
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Carsguide\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Carsguide\\ReCaptcha\\Providers\\ReCaptchaServiceProvider"
            ]
        }
    }
}
